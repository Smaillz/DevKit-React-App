  "description": "My devkit react app(typescript)",
  "repository": {},
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "clean:test": "rm -rf coverage",
    "prod": "npm run clean && npm run lint && npm run build && npm run zip",
    "build": "npm run clean && webpack --config ./webpack.config.prod.js --env.NODE_ENV=production",
    "start": "npm run lint && webpack-dev-server --open 'Google Chrome' --env.NODE_ENV=development",
    "start:win": "npm run lint && webpack-dev-server --open chrome --env.NODE_ENV=development",
    "lint": "tslint src/**/*.ts* --force",
    "stylelint": "stylelint src/**/*.s*ss --fix",
    "test": "jest --colors",
    "test:dev": "jest --watch",
    "serve": "node ./server/index.js",
    "zip": "node ./scripts/zip.js"
  },
